openssl req -x509 -nodes -days 3650 -subj /CN=Barrier -newkey rsa:4096 -keyout ~/.local/share/barrier/SSL
/Barrier.pem -out ~/.local/share/barrier/SSL/Barrier.pem
mkdir ~/.local/share/barrier/SSL/Fingerprints
openssl x509 -fingerprint -sha2 -noout -in ~/.local/share/barrier/SSL/Barrier.pem > ~/.local/share/barrier/SSL/Fingerprints/Local.txt
